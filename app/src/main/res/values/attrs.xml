<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name = "TestBox" >
        <attr name="box_image" format="reference|integer"/>
        <attr name="box_name" format="string"/>
        <attr name="box_position" format="integer"/>
        <attr name="box_score" format="string" />
    </declare-styleable>
    <declare-styleable name = "RoundImageView" >
        <attr name="image_radius" format="integer"/>
    </declare-styleable>
    <declare-styleable name = "MuseumCard" >
        <attr name="museum_card_name" format="string"/>
        <attr name="museum_card_image" format="reference|integer"/>
        <attr name="museum_card_score" format="string"/>
        <attr name="museum_card_text" format="string"/>
    </declare-styleable>
    <declare-styleable name="MapRecentCard">
        <attr name="map_museum_name" format="string"/>
        <attr name="map_museum_position" format="string"/>
        <attr name="map_museum_image" format="reference|integer"/>
        <attr name="map_museum_info" format="string"/>
    </declare-styleable>
    <declare-styleable name="AppraiseScore">
        <attr name="appraise_score_number" format="integer"/>
        <attr name="appraise_score_click" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AppraiseCard">
        <attr name="appraise_name" format="string"/>
        <attr name="appraise_time" format="string"/>
        <attr name="appraise_image" format="reference|integer"/>
        <attr name="appraise_comment" format="string"/>
        <attr name="appraise_score" format="integer"/>
    </declare-styleable>
</resources>
